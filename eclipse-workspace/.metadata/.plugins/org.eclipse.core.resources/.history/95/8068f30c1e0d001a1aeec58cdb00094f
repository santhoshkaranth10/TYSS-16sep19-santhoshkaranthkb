package com.testyantra.jpawithhibernate.manytomany;

import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.Id;
import javax.persistence.Table;

import lombok.Data;

@Entity
@Data
@Table(name="course")
public class Course {

	
	@Id
	@Column(name="cid")
	private int cid;
	
	@Column(name="cname")
	private String cname;
	
}
