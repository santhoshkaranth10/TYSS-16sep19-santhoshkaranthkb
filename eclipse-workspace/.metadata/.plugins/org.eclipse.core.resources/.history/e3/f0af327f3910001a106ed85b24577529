package com.testyantra.springcore.di;

public class Hello {

	public Hello() {
		
	}
	private String msg;

	public String getMsg() {
		return msg;
	}

	public void setMsg(String msg) {
		this.msg = msg;
	}
	
	public void init() {
		System.out.println("init method executing from hello");
	}
	public void destroy() {
		System.out.println("destroy method executing from java");
	}
}